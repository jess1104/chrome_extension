{
    "manifest_version": 2,
    "name": "JessWu Tools",
    "version": "1.0.0",
    "description": "Chrome 插件 demo",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["new_fresh.js"],
        "run_at": "document_start"
    }],
    "icons": {
        "16": "/images/logo16.png",
        "32": "/images/logo32.png",
        "64": "/images/logo64.png",
        "128": "/images/logo128.png"
      }

 }
